import java.util.*;

public class Main {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int a;
		Scanner input = new Scanner(System.in);
		a = input.nextInt();
		
		System.out.print(~(a % 2) + 2);
		System.out.print(~((a / 2) % 2) + 2);
		System.out.print(~(((a / 2) / 2) % 2) + 2);
		System.out.print(~((((a / 2) / 2) / 2) % 2) + 2);
	}

}
